{
    "manifest_version": 1,
    
    "name": "MediaHack Rutube improver",
    "description": "This extension was made by Klimov",
    "version": "1.0",
    
    "browser_action": {
        "default_title": "MediaHack Fingerprint"
    },
    
    "permissions": ["tabs", "activeTab", "http://*/*"],
    
    "content_scripts" : [{
                         "matches" : [ "*://rutube.ru/"],
                         "js" : ["rutube.user.js"]
                         }]
}
